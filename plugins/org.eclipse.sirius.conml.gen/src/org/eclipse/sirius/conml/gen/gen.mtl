[comment encoding = UTF-8 /]
[module gen('http://conml.ecore')/]

[import org::eclipse::sirius::conml::gen::file::css::styles/]

[import org::eclipse::sirius::conml::gen::file::js::classListJs/]
[import org::eclipse::sirius::conml::gen::file::js::partsJs/]
[import org::eclipse::sirius::conml::gen::file::js::findJs/]
[import org::eclipse::sirius::conml::gen::file::js::jQuery/]

[import org::eclipse::sirius::conml::gen::file::html::classPage/]
[import org::eclipse::sirius::conml::gen::file::html::classListPage/]
[import org::eclipse::sirius::conml::gen::file::html::enumTypeListPage/]
[import org::eclipse::sirius::conml::gen::file::html::enumTypePage/]
[import org::eclipse::sirius::conml::gen::file::html::findPage/]

[template public gen(aDomain : Domain, projectName : String)]
[comment @main/]
[styles()/]
[findPage(projectName)/]
[classListJs()/]
[classListPage(aDomain, projectName)/]
[enumTypeListPage(aDomain, projectName)/]
[for (aClass : Class | aDomain.eAllContents(Class))]
	[classPage(aClass)/]
[/for]
[for (anEnumType : EnumeratedType | aDomain.eAllContents(EnumeratedType))]
	[enumTypePage(anEnumType)/]
[/for]
[partsJs(aDomain)/]
[findJs()/]
[jQuery()/]
[/template]
